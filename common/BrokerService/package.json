{
  "name": "broker-service",
  "version": "0.0.1",
  "description": "Intermediary lib to route API requests to microservices",
  "main": "dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json"
  },
  "keywords": [
    "yape",
    "broker",
    "api",
    "lib"
  ],
  "author": "Geovanny G. Arguello",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "dotenv": "^16.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.6",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "typescript": "^5.3.3"
  }
}
